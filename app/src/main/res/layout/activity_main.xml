<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cream"
    tools:context=".MainActivity">

   <FrameLayout
       android:id="@+id/container"
       android:layout_width="match_parent"
       android:layout_height="0dp"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintBottom_toTopOf="@id/Bottom_bar"/>
    <TextView
        android:id="@+id/title2"
        android:layout_margin="16dp"
        android:textColor="@color/black"
        android:textSize="@dimen/dim16"
        android:text="@string/my_family"
        android:background="@color/cream"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>


    <ImageView
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_margin="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/baseline_local_florist_24"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_margin="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/twotone_menu_24"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.card.MaterialCardView
        app:cardElevation="@dimen/dim_16_dp"
        android:id="@+id/card1"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_margin="16dp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="16dp"
        app:layout_constraintTop_toBottomOf="@+id/title2"
        tools:ignore="UnknownId" >

        <TextView
            android:id="@+id/text_sos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dim_16_dp"
            android:elevation="16dp"

            android:textColor="#151414"
            android:textSize="22sp"
            android:textStyle="bold"

            />
    </com.google.android.material.card.MaterialCardView>
        <ImageView
            android:id="@+id/imag1"
         android:layout_height="76dp"
            android:layout_width="76dp"
            android:src="@drawable/img_2"
            android:elevation="@dimen/dim_16_dp"
            app:layout_constraintTop_toTopOf="@+id/card1"
            app:layout_constraintStart_toStartOf="@id/card1"
            android:layout_margin="16dp"/>

    <com.google.android.material.card.MaterialCardView
        app:cardElevation="@dimen/dim_16_dp"
        android:id="@+id/card2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_margin="16dp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="16dp"
        app:layout_constraintTop_toBottomOf="@+id/card1" />

    <ImageView
        android:id="@+id/imag2"
        android:layout_height="80dp"
        android:layout_width="80dp"
        android:src="@drawable/img_3"
        android:elevation="@dimen/dim_16_dp"
        app:layout_constraintTop_toTopOf="@+id/card2"
        app:layout_constraintStart_toStartOf="@id/card2"
        android:layout_margin="16dp"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="@dimen/dim16"
        android:id="@+id/n2"
        android:layout_margin="16dp"
        android:text="Tanisha Mangla"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/card2"
        app:layout_constraintStart_toEndOf="@id/imag2"
        app:layout_constraintTop_toBottomOf="@id/title2"/>

    <com.google.android.material.card.MaterialCardView
        app:cardElevation="@dimen/dim_16_dp"
        android:id="@+id/card3"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_margin="16dp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="16dp"
        app:layout_constraintTop_toBottomOf ="@+id/card2" />
    <ImageView
        android:id="@+id/imag3"
        android:layout_height="76dp"
        android:layout_width="76dp"
        android:src="@drawable/img_4"
        android:elevation="@dimen/dim_16_dp"
        app:layout_constraintTop_toTopOf="@+id/card3"
        app:layout_constraintStart_toStartOf="@id/card3"
        android:layout_margin="16dp"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="@dimen/dim16"
        android:id="@+id/n3"
        android:layout_margin="16dp"
        android:text="Mamta Mangla"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/card3"
        app:layout_constraintStart_toEndOf="@id/imag3"
        app:layout_constraintTop_toBottomOf="@id/title2"/>


    <com.google.android.material.card.MaterialCardView
        app:cardElevation="@dimen/dim_16_dp"
        android:id="@+id/card4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/cardmargin"
        android:layout_margin="16dp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="16dp"
        app:layout_constraintTop_toBottomOf="@+id/card3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="@dimen/dim16"
        android:id="@+id/num_a"
        android:layout_margin="16dp"
         android:text="Aakash Mangla"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@id/card1"
        app:layout_constraintStart_toEndOf="@id/imag1"
        app:layout_constraintTop_toBottomOf="@id/title2"/>



    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/Bottom_bar"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemIconTint="@drawable/nav_item_colors"
        app:itemTextColor="@color/lightgreen"
        app:itemBackground="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/bottom_bar_item"
        />
</androidx.constraintlayout.widget.ConstraintLayout>